<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="bonch.dev.view.regulardrive.RegularDriveFragment"
        android:label="@string/title_home"
        tools:layout="@layout/regular_drive_fragment" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="bonch.dev.view.getdriver.GetDriverView"
        android:label="@string/title_dashboard"
        tools:layout="@layout/get_driver_fragment" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="bonch.dev.view.profile.ProfileFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/profile_fragment" />
</navigation>